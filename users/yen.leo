# create a Chord table entry
def createChordtableEntry(conn, key, tuningname, chordname, alt_chordname, comment, generator, version, fretposstring, SVG, derivation):
    cursor = conn.cursor()
    # get id of key - I separated these into sub-queries to simplify the SQL
    sqls = "SELECT idkey FROM `key` WHERE keyname LIKE \'" + key + "\'\n"
    cursor.execute(sqls)
    row = cursor.fetchone()
    idkey = row[0]
    # get id of tuning
    sqls = "SELECT idtuning FROM `tuning` WHERE tuningname LIKE \'" + tuningname + "\'\n"
    cursor.execute(sqls)
    row1 = cursor.fetchone()
    idtuning = row1[0]
    
    sqls = """INSERT INTO `chord` (derivation, idkey, idtuning, chordname, alt_chordname, comment, generator, version, fretposstring, SVG)
            VALUES ('""" + derivation + """',""" + str(idkey) + """,""" + str(idtuning) + """,'""" + chordname + """','""" + alt_chordname + """','""" + comment + """','""" + generator + """','""" + version + """','""" + fretposstring + """','""" + re.escape(SVG) + """')\n"""
    cursor.execute(sqls)
    conn.commit()
    

